{
  "host": "localhost",
  "port": 8080,
  "cacheTime": "20m",
  "cachePath": ".jr/cache/",
  "substitutePath": ".jr/subs/",
  "jrPort": 8000,
  "proxyTimeout": "6s",
  "rules": [
    {
      "href": ".+",
      "keepFresh": true
    },
    {
      "href": "getUsersList.do",
      "subs": "getUsersList.jrs"
    }
  ]
}
